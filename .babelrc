{
    "plugins": [
			"transform-decorators-legacy",
			"syntax-decorators",
			"transform-object-rest-spread"
    ],
    "presets": [
      ["es2015", { "modules": false }],
      "stage-0",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": ["react-optimize"]
      }
    }
  }
